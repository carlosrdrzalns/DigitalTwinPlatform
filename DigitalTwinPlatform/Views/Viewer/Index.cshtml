@model DigitalTwinPlatform.Models.Viewer

<script>
    $(document).ready(async function () {

            var urn = '@Model.urn';
            var user = '@Model.User';
        var viewableId = '@Model.viewableId';
        var modelName = '@Model.modelName';
        launchViewer(urn, modelName, user, viewableId);
        getUseCases('@Model.ModelId.ToString()');
    });
</script>
@{
    Layout = "_Layout";
}
